{
  "name": "@neurosonix/signal-gen-backend",
  "version": "1.0.0",
  "description": "NeuroSonix Industrial Backend - TypeScript Fastify Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "deploy": "node deploy-real-api.mjs",
    "real-api": "npm run build && npm start"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2",
    "tsx": "^3.14.0",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4"
  },
  "keywords": [
    "neurosonix",
    "industrial",
    "fastify", 
    "typescript",
    "albi",
    "alba", 
    "jona",
    "real-data"
  ],
  "author": "NeuroSonix Industrial Team",
  "license": "MIT"
}