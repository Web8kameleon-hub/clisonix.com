{
  "dashboard": {
    "title": "ðŸ§  Clisonix AI Agents - Real-Time Monitoring",
    "uid": "clisonix-ai-agents",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "AI Agent Frameworks Status",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "up{job='ai-agents'}",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4}
      },
      {
        "id": 2,
        "title": "CrewAI Agent Activity",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(crewai_tasks_completed_total[5m])",
            "legendFormat": "Tasks/sec"
          },
          {
            "expr": "rate(crewai_tasks_failed_total[5m])",
            "legendFormat": "Failures/sec"
          }
        ],
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4}
      },
      {
        "id": 3,
        "title": "LangChain Conversation Chains",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(langchain_chains_executed_total[5m])",
            "legendFormat": "Chains/sec"
          },
          {
            "expr": "langchain_active_conversations",
            "legendFormat": "Active Conversations"
          }
        ],
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4}
      },
      {
        "id": 4,
        "title": "Claude API Requests",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(claude_api_requests_total[5m])",
            "legendFormat": "Req/sec"
          }
        ],
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4}
      },
      {
        "id": 5,
        "title": "API Response Times (All Agents)",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 4}
      },
      {
        "id": 6,
        "title": "ALBA Network Metrics",
        "type": "table",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "alba_network_connections",
            "format": "table",
            "instant": true
          },
          {
            "expr": "alba_cpu_usage_percent",
            "format": "table",
            "instant": true
          },
          {
            "expr": "alba_memory_usage_mb",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 4}
      },
      {
        "id": 7,
        "title": "ALBI EEG Analysis - Frequency Bands",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "albi_eeg_delta_band",
            "legendFormat": "Delta"
          },
          {
            "expr": "albi_eeg_theta_band",
            "legendFormat": "Theta"
          },
          {
            "expr": "albi_eeg_alpha_band",
            "legendFormat": "Alpha"
          },
          {
            "expr": "albi_eeg_beta_band",
            "legendFormat": "Beta"
          },
          {
            "expr": "albi_eeg_gamma_band",
            "legendFormat": "Gamma"
          }
        ],
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 4}
      },
      {
        "id": 8,
        "title": "JONA Neural Synthesis - Performance",
        "type": "gauge",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "jona_synthesis_confidence_score",
            "legendFormat": "Confidence"
          }
        ],
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 4}
      },
      {
        "id": 9,
        "title": "OpenAI API Status",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "openai_api_available",
            "legendFormat": "Status"
          }
        ],
        "gridPos": {"x": 0, "y": 12, "w": 6, "h": 3}
      },
      {
        "id": 10,
        "title": "Error Rates by Component",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~'5..'}[5m])",
            "legendFormat": "{{component}}"
          }
        ],
        "gridPos": {"x": 6, "y": 12, "w": 18, "h": 4}
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
