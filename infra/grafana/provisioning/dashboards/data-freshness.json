{
  "title": "Data Freshness & Concept Gaps",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "CrossRef Last Success (s)",
      "targets": [
        {
          "expr": "time() - max(airflow_dag_last_success{dag=\"crossref_ingest\"})"
        }
      ],
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 0}
    },
    {
      "type": "stat",
      "title": "Clinical Trials Rows",
      "targets": [
        {
          "expr": "pg_table_rows{table=\"clinical_trials\"}"
        }
      ],
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 0}
    }
  ],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"}
}
