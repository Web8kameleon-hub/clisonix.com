{
  "info": {
    "name": "Clisonix Cloud - AI Agents API Testing",
    "description": "Complete Postman collection for testing all AI agent framework endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "2.0",
    "date": "2025-12-10"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{openai_api_key}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "openai_api_key",
      "value": "sk-your-key-here",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "AI Agent Frameworks",
      "item": [
        {
          "name": "1. Get Agents Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/ai/agents-status",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "agents-status"]
            },
            "description": "Get status of all AI agent frameworks (CrewAI, LangChain, Claude Tools)"
          },
          "response": []
        },
        {
          "name": "2. CrewAI Trinity Analysis",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"Analyze ALBA network performance and recommend optimizations\",\n  \"detailed\": true\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/ai/trinity-analysis",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "trinity-analysis"]
            },
            "description": "Use CrewAI to coordinate ALBA, ALBI, JONA agents for comprehensive analysis"
          },
          "response": []
        },
        {
          "name": "3. LangChain Curiosity Ocean - First Question",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"How does neural synthesis work in the context of consciousness?\",\n  \"conversation_id\": \"conv-{{$timestamp}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/ai/curiosity-ocean",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "curiosity-ocean"]
            },
            "description": "Start a multi-turn conversation with LangChain memory for deep exploration"
          },
          "response": []
        },
        {
          "name": "4. LangChain Curiosity Ocean - Follow-up Question",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"Can you explain how this relates to quantum effects in the brain?\",\n  \"conversation_id\": \"{{conversation_id}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/ai/curiosity-ocean",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "curiosity-ocean"]
            },
            "description": "Continue conversation - AI remembers previous context"
          },
          "response": []
        },
        {
          "name": "5. Claude Tools - Quick Interpretation",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"What does alpha wave dominance (>50%) indicate about cognitive state?\",\n  \"context\": \"EEG frequency analysis at rest\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/ai/quick-interpret",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "quick-interpret"]
            },
            "description": "Quick Claude interpretation without orchestration overhead - fast response time"
          },
          "response": []
        }
      ]
    },
    {
      "name": "Real Data Endpoints",
      "item": [
        {
          "name": "1. ASI Trinity Status (Real Data)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/asi/status",
              "host": ["{{base_url}}"],
              "path": ["api", "asi", "status"]
            },
            "description": "Get real metrics from Prometheus for ALBA, ALBI, JONA"
          },
          "response": []
        },
        {
          "name": "2. Crypto Prices (CoinGecko Real)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/external/crypto",
              "host": ["{{base_url}}"],
              "path": ["api", "external", "crypto"]
            },
            "description": "Real cryptocurrency prices from CoinGecko API"
          },
          "response": []
        },
        {
          "name": "3. Weather Data (Open-Meteo Real)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/external/weather",
              "host": ["{{base_url}}"],
              "path": ["api", "external", "weather"]
            },
            "description": "Real weather data from Open-Meteo for multiple cities"
          },
          "response": []
        }
      ]
    },
    {
      "name": "OpenAI Neural Analysis",
      "item": [
        {
          "name": "1. Analyze Neural Data (GPT-4)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"Analyze alpha wave patterns in relaxed state. What neural processes might be active?\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/ai/analyze-neural?query=Analyze%20alpha%20wave%20patterns%20in%20relaxed%20state",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "analyze-neural"],
              "query": [
                {
                  "key": "query",
                  "value": "Analyze alpha wave patterns in relaxed state"
                }
              ]
            },
            "description": "Real OpenAI GPT-4 analysis (requires valid API key)"
          },
          "response": []
        },
        {
          "name": "2. EEG Interpretation (GPT-4)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"frequencies\": {\n    \"delta\": 0.5,\n    \"theta\": 1.2,\n    \"alpha\": 8.5,\n    \"beta\": 15.3,\n    \"gamma\": 35.0\n  },\n  \"dominant_freq\": 8.5,\n  \"amplitude_range\": {\n    \"min\": 0.1,\n    \"max\": 2.5\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/ai/eeg-interpretation",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "eeg-interpretation"]
            },
            "description": "Real OpenAI EEG signal interpretation using GPT-4"
          },
          "response": []
        },
        {
          "name": "3. OpenAI API Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/ai/health",
              "host": ["{{base_url}}"],
              "path": ["api", "ai", "health"]
            },
            "description": "Check OpenAI API connectivity and configuration status"
          },
          "response": []
        }
      ]
    },
    {
      "name": "Monitoring & Prometheus",
      "item": [
        {
          "name": "1. Real Metrics Info",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/monitoring/real-metrics-info",
              "host": ["{{base_url}}"],
              "path": ["api", "monitoring", "real-metrics-info"]
            },
            "description": "Documentation of real Prometheus metrics being collected"
          },
          "response": []
        },
        {
          "name": "2. System Health",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/health",
              "host": ["{{base_url}}"],
              "path": ["health"]
            },
            "description": "Overall system health check"
          },
          "response": []
        },
        {
          "name": "3. System Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/status",
              "host": ["{{base_url}}"],
              "path": ["status"]
            },
            "description": "Detailed system status and service information"
          },
          "response": []
        }
      ]
    }
  ]
}
